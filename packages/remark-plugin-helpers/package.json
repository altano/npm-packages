{
  "name": "@altano/remark-plugin-helpers",
  "version": "0.0.1",
  "private": true,
  "description": "Utilities for remark plugins",
  "type": "module",
  "sideEffects": false,
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
