  },
  "author": {
    "name": "Alan Norbauer",
    "email": "altano@gmail.com",
    "url": "https://alan.norbauer.com"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/altano/npm-packages",
    "directory": "packages/remark-plugin-helpers"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@altano/tsconfig": "*",
    "@types/node": "^18.6.4",
    "@types/prettier": "^2.7.0",
    "prettier": "^2.7.1",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "kleur": "^4.1.5",
    "unist-util-visit": "^4.1.0"
