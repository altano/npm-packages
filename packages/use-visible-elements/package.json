{
  "name": "@altano/use-visible-elements",
  "version": "1.0.5",
  "description": "Intersection-Observer-based Scroll Spy React Hook",
  "type": "module",
  "sideEffects": false,
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm --dts",
    "dev": "tsup src/index.tsx --format esm --watch --dts",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "format": "prettier --write src",
    "clean": "rm -rf  && rm -rf .tsbuildinfo && rm -rf node_modules && rm -rf dist"
  },
  "author": {
    "name": "Alan Norbauer",
    "email": "altano@gmail.com",
    "url": "https://alan.norbauer.com"
  },
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/altano/npm-packages",
    "directory": "packages/use-visible-elements"
  },
  "dependencies": {
    "@altano/use-element-observer": "*",
    "react-use": "^17.4.0"
  },
  "peerDependencies": {
    "react": "^18"
  },
  "devDependencies": {
    "@altano/tsconfig": "*",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.20.0",
    "eslint-config-altano": "*",
    "prettier": "^2.7.1",
    "react": "^18",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "publishConfig": {
    "access": "public"
  }
