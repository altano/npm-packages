{
  "name": "@altano/use-toc-visible-sections",
  "version": "1.0.5",
  "description": "Intersection-Observer-based Table of Contents Scroll Spy React Hook",
  "type": "module",
  "sideEffects": false,
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm --dts",
    "dev": "tsup src/index.ts --watch --format esm --dts",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "format": "prettier --write src",
    "clean": "rm -rf .turbo && rm -rf .tsbuildinfo && rm -rf node_modules && rm -rf dist"
  },
  "author": {
    "name": "Alan Norbauer",
    "email": "altano@gmail.com",
    "url": "https://alan.norbauer.com"
  },
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/altano/npm-packages",
    "directory": "packages/use-toc-visible-sections"
  },
  "homepage": "https://github.com/altano/npm-packages/tree/main/packages/use-toc-visible-sections",
  "dependencies": {
    "@altano/use-visible-elements": "workspace:*"
  },
  "devDependencies": {
    "@altano/tsconfig": "workspace:*",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "eslint": "^8.39.0",
    "eslint-config-altano": "workspace:*",
    "prettier": "^2.8.7",
    "react": "^18",
    "tsup": "^6.7.0",
    "typescript": "^5.1.6"
  },
  "publishConfig": {
    "access": "public"
  }
