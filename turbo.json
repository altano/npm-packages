{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["CI"],
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next/**"],
      "dependsOn": ["lint", "^build"]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": ["build", "^test"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
